{
  "name": "@strangemood/cli",
  "version": "0.0.3",
  "description": "CLI for the Strangemood Protocol",
  "author": "Evan Conrad",
  "bin": {
    "strangemood": "./bin/run"
  },
  "homepage": "https://github.com/strangemoodfoundation/strangemood",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "strangemoodfoundation/strangemood",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@ipld/car": "^3.2.3",
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-plugins": "^2.0.1",
    "@project-serum/anchor": "^0.22.0",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-governance": "^0.0.19",
    "@solana/spl-token": "0.2.0-alpha.2",
    "@solana/web3.js": "^1.34.0",
    "@strangemood/strangemood": "0.0.0-88",
    "carbites": "^1.0.6",
    "chalk": "^4.1.2",
    "ipfs-car": "^0.6.2",
    "ora": "5.4.1",
    "precrypt": "^0.3.6",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "oclif": {
    "bin": "strangemood",
    "dirname": "strangemood",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "charter": {
        "description": "Create and manage governances"
      },
      "listing": {
        "description": "Create and manage listings"
      },
      "receipt": {
        "description": "Create and manage in-progress purchases"
      },
      "treasury": {
        "description": "Create and manage currency support"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/strangemoodfoundation/strangemood/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
